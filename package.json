{
  "name": "e-commerce",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://zetahut.com",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "dotenv": "^16.0.0",
    "framer-motion": "^6.3.3",
    "ftp-deploy": "^2.4.1",
    "npm-watch": "^0.11.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "react-scripts": "5.0.0",
    "styled-components": "^5.3.3",
    "typewriter-effect": "^2.18.2",
    "watch": "^1.0.2",
    "web-vitals": "^2.1.2",
    "yarn": "^1.22.17"
  },
  "watch": {
    "build": {
      " patterns": [
        "src"
      ],
      "extensions": "js,jsx"
    }
  },
  "scripts": {
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "cleanBuild": "rimraf ./build/*",
    "watch": "npm watch",
